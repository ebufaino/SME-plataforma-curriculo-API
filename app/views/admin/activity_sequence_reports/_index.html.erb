<% context.instance_eval do
  column :title do |activity_sequence|
    link_to activity_sequence.title, admin_activity_sequence_path(activity_sequence.id), target: :_blank
  end
  Rating.enableds.each do |rating|
    column rating.description.to_sym do |activity_sequence|
      number_with_precision(activity_sequence.average_by_rating_type(rating.id), precision: 2)
    end
  end
  column :total_evaluations do |activity_sequence|
    activity_sequence.total_evaluations
  end
end %>