<legend><span>Galeria</span></legend>
<% f.input :id, as: :hidden %>
<% f.input :content_block_id, as: :hidden, input_html: { value: ContentBlock.find_by(content_type: :gallery).id  }%>
<% f.has_many :images do |ff| %>
<%   ff.input :subtitle, required: true %>
<%   ff.input :file, required: true, as: :file %>
<% end %>
<% f.input :_destroy, as: :boolean unless f.object.new_record? %>